{
  "data": [
    {
      "name": "video_source",
      "type": "inputNode",
      "parameters": [
        {
          "name": "path",
          "flag": "-i",
          "type": "str",
          "default": "example-video.mp4",
          "required": true,
          "description":"Input file path"
        }
      ]
    },
    {
      "name": "color",
      "type": "processNode",
      "parameters": [
        {
          "name": "input_colorspace",
          "flag": "-ic",
          "type": "str",
          "default": "RGB",
          "required": true,
          "options": [
            "YCbCr",
            "HSV",
            "Lab",
            "RGB"
          ],
          "description":"Input color space"
        },
        {
          "name": "output_colorspace",
          "flag": null,
          "type": "str",
          "default": "RGB",
          "required": true,
          "options": [
            "YCbCr",
            "HSV",
            "Lab",
            "RGB"
          ],
          "description":"Input color space"
        }
      ]
    },
    {
      "name": "blur",
      "type": "processNode",
      "parameters": [
        {
          "name": "kernel_size",
          "flag": "-oc",
          "type": "int",
          "default": 5,
          "min": 1,
          "max": 31,
          "required": true,
          "description":"Kernel size (must be odd)"
        },
        {
          "name": "method",
          "flag": null,
          "type": "str",
          "required": true,
          "default": "gaussian",
          "options": ["gaussian", "median", "average"],
          "description": "Blur algorithm type"
        }
      ]
    },
    {
      "name": "resize",
      "type": "processNode",
      "parameters": [
        {
          "name": "width",
          "flag": null,
          "type": "int",
          "default": 640,
          "min": 32,
          "max": 3840,
          "required": true,
          "description": "Output width in pixels"
        },
        {
          "name": "height",
          "flag": null,
          "type": "int",
          "default": 480,
          "min": 32,
          "max": 2160,
          "required": true,
          "description": "Output height in pixels"
        },
        {
          "name": "interpolation",
          "flag": null,
          "type": "str",
          "required": true,
          "default": "linear",
          "options": ["linear", "nearest", "cubic", "area"],
          "description": "Interpolation method"
        }
      ]
    },
    {
      "name": "video_output",
      "type": "outputNode",
      "parameters": [
        {
          "name": "video_player",
          "flag": null,
          "type": "str",
          "default": "right",
          "required": true,
          "options": ["left", "right"],
          "description": "Output Video Player"
        }
      ]
    },
    {
      "name": "simple-video-processor-app",
      "type": "processNode",
      "executable": "simple-video-processor-app",
      "parameters": [
        {
          "name": "input",
          "flag": "-i",
          "type": "str",
          "required": true
        },
        {
          "name": "width",
          "flag": "-w",
          "type": "int",
          "required": true
        },
        {
          "name": "height",
          "flag": "-h",
          "type": "int",
          "required": true
        },
        {
          "name": "mode",
          "flag": "-m",
          "type": "int",
          "required": true
        },
        {
          "name": "operator",
          "flag": "--operator",
          "type": "int",
          "required": true
        },
        {
          "name": "y",
          "flag": "--y",
          "type": "int",
          "required": true
        },
        {
          "name": "u",
          "flag": "--u",
          "type": "int",
          "required": true
        },
        {
          "name": "v",
          "flag": "--v",
          "type": "int",
          "required": true
        },
        {
          "name": "output",
          "flag": "-o",
          "type": "str",
          "required": true
        },
        {
          "name": "verbose",
          "flag": "--verbose",
          "type": "bool",
          "required": false,
          "default": false
        }
      ]
    }
  ]
}