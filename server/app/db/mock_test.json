      "parameters": [
        {
          "name": "path",
          "flag": "-i",
          "type": "str",
          "default": "example-video.mp4",
          "required": true
        }
      ]
    },
    {
      "name": "color_space",
      "type": "processNode",
      "parameters": [
        {
          "name": "input_colorspace",
          "flag": "-ic",
          "type": "str",
          "default": "RGB",
          "required": true,
          "options": ["RGB", "YCrCb", "HSV", "GRAY"]
        },
        {
          "name": "output_colorspace",
          "flag": null,
          "type": "str",
          "default": "RGB",
          "required": true,
          "options": ["RGB", "YCrCb", "HSV", "GRAY"]
        }
      ]
    },
    {
      "name": "blur",
      "type": "processNode",
      "parameters": [
        {
          "name": "kernel_size",
          "flag": "-oc",
          "type": "int",
          "default": 5,
          "required": true
        },
        {
          "name": "method",
          "flag": null,
          "type": "str",
          "required": true,
          "default": "gaussian",
          "options": ["gaussian", "median", "average"]
        }
      ]
    },
    {
      "name": "resize",
      "type": "processNode",
      "parameters": [
        {
          "name": "width",
          "flag": null,
          "type": "int",
          "default": 640,
          "required": true
        },
        {
          "name": "height",
          "flag": null,
          "type": "int",
          "default": 480,
          "required": true
        },
        {
          "name": "interpolation",
          "flag": null,
          "type": "str",
          "required": true,
          "default": "linear",
          "options": ["linear", "nearest", "cubic", "area"]
        }
      ]
    },
    {
      "name": "result",
      "type": "outputNode",
      "parameters": [
        {
          "name": "video_player",
          "flag": null,
          "type": "str",
          "default": "right",
          "options": ["left", "right"],
          "required": true
        }
      ]
    },
    {
      "name": "simple-video-processor-app",
      "type": "processNode",
      "executable": "simple-video-processor-app",
      "parameters": [
        {
          "name": "input",
          "flag": "-i",
          "type": "str",
          "required": true
        },
        {
          "name": "width",
          "flag": "-w",
          "type": "int",
          "required": true
        },
        {
          "name": "height",
          "flag": "-h",
          "type": "int",
          "required": true
        },
        {
          "name": "mode",
          "flag": "-m",
          "type": "int",
          "required": true
        },
        {
          "name": "operator",
          "flag": "--operator",
          "type": "int",
          "required": true
        },
        {
          "name": "y",
          "flag": "--y",
          "type": "int",
          "required": true
        },
        {
          "name": "u",
          "flag": "--u",
          "type": "int",
          "required": true
        },
        {
          "name": "v",
          "flag": "--v",
          "type": "int",
          "required": true
        },
        {
          "name": "output",
          "flag": "-o",
          "type": "str",
          "required": true
        },
        {
